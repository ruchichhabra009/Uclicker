 <style type="text/css">
  html, body {height: 100%;}
  #wrap {min-height: 100%;}

  #main {overflow:auto;
    padding-bottom: 25px;} 
    body {
      padding-top: 100px;
      padding-bottom: 20px;
    }
    body.loginbody {
      padding-top: 140px;
      padding-bottom: 20px;
    }
    .active, .passive {
      margin-top:20px;
    }
    .form-group {
      margin-top:20px;
    }
    .btn-primary {
      margin-top:20px;
    }
    .jumbotron {
      text-align: center;
    }
    
    .navbar-fixed-top{
      border-bottom: 1px solid #c9aa5f;
